<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MiniGauge">
        <attr name="checked_color" format="color" />
        <attr name="unchecked_color" format="color" />
        <attr name="mini_label" format="string" />
        <attr name="mini_title" format="string" />
        <attr name="mini_subtitle" format="string" />
        <attr name="message" format="string" />
        <attr name="min_value" format="float" />
        <attr name="max_value" format="float" />
        <attr name="default_value" format="float" />
    </declare-styleable>


    <declare-styleable name="Gauge">
        <attr name="gauge_inactive_color" format="color" />
        <attr name="gauge_active_color" format="color" />
        <attr name="gauge_stroke_width_active" format="integer" />
        <attr name="gauge_stroke_width_inactive" format="integer" />
        <attr name="gauge_animate_set" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PieChart">
        <attr name="autoCenterPointerInSlice" format="boolean" />
        <attr name="highlightStrength" format="float" />
        <attr name="labelColor" format="color" />
        <attr name="labelHeight" format="dimension" />
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="labelWidth" format="dimension" />
        <attr name="labelY" format="dimension" />
        <attr name="pieRotation" format="integer" />
        <attr name="pointerRadius" format="dimension" />
        <attr name="showTextPie" format="boolean" />
    </declare-styleable>

</resources>